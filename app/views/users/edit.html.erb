<%= form_for current_user, :html => {:multipart => true} do |f| %>
  
<p>
  <%= f.file_field :image %>
</p>


  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </div>
 
  <div class="actions">
    <button type="button" class="btn btn-info">
      <%= f.submit "Update User" %></button>
    <%= link_to "Back", user_tweets_path(current_user) %>
  </div>
  

<% end %>